): Array<Array<String>> {
  let handler = new __near_JSONHandler_Array_Array_String();
  handler.buffer = buffer;
  handler.decoder = new JSONDecoder<__near_JSONHandler_Array_Array_String>(
    handler
  );
  handler.decoder.deserialize(buffer, state);
